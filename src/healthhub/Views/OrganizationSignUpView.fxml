<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="healthhub.ViewControllers.OrganizationSignUpController">
    <children>
        <Rectangle fx:id="Email" arcHeight="5.0" arcWidth="5.0" fill="#2a0033" height="400.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
        <Label layoutX="119.0" layoutY="68.0" prefHeight="30.0" prefWidth="102.0" text="Owner Name" textFill="#c9c1c1">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="119.0" layoutY="121.0" prefHeight="32.0" prefWidth="118.0" text="Owner Age" textFill="#c9c1c1">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label layoutX="119.0" layoutY="174.0" prefHeight="32.0" prefWidth="132.0" text="Phone Number"
               textFill="#c9c1c1">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label layoutX="119.0" layoutY="227.0" prefHeight="32.0" prefWidth="132.0" text="Owner Email"
               textFill="#c9c1c1">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label layoutX="119.0" layoutY="280.0" prefHeight="32.0" prefWidth="124.0" text="Owner Password"
               textFill="#c9c1c1">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="ownerName" layoutX="288.0" layoutY="68.0" promptText="First Name"/>
        <TextField fx:id="age" layoutX="288.0" layoutY="121.0"/>
        <TextField fx:id="phoneNumber" layoutX="288.0" layoutY="174.0"/>
        <TextField fx:id="email" layoutX="288.0" layoutY="227.0" promptText="example@example.ca"/>
        <TextField fx:id="Password" layoutX="288.0" layoutY="280.0"/>
        <Button layoutX="314.0" layoutY="323.0" mnemonicParsing="false" onAction="#onCreateOrganizationButtonPushed"
                prefHeight="33.0" prefWidth="180.0"
                style="-fx-border-color: #9643A9; -fx-border-radius: 10;                 -fx-background-color: #2A0033; -fx-background-radius: 10;"
                text="Create Organization" textFill="#ddd9d9"/>

        <TextField fx:id="organizationName" layoutX="288.0" layoutY="15.0"/>

        <Label layoutX="119.0" layoutY="13.0" prefHeight="30.0" prefWidth="138.0" text="Organization Name"
               textFill="#c9c1c1">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Button layoutX="108.0" layoutY="323.0" mnemonicParsing="false" onAction="#backButtonPushed" prefHeight="33.0"
                prefWidth="180.0"
                style="-fx-border-color: #9643A9; -fx-border-radius: 10;                 -fx-background-color: #2A0033; -fx-background-radius: 10;"
                text="Back" textFill="#ddd9d9"/>
    </children>
</AnchorPane>
