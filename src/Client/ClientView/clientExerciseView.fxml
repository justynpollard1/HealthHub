            <VBox fx:id="exerciseSetsVBox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="Sets" textFill="#c1c9c9"/>
               </children>
            </VBox>
            <VBox fx:id="exerciseRepsVBox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="Reps" textFill="#c1c9c9"/>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
