<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="Client.ClientView.ExerciseSearch.ExerciseSearchViewController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#2a0033" height="400.0" stroke="BLACK" strokeType="INSIDE"
                 width="600.0"/>
      <TextField fx:id="searchBar" alignment="CENTER" layoutX="207.0" layoutY="14.0" promptText="Search for exercise"
                 style="-fx-background-color: #ffffff; -fx-border-color: #9643A9; -fx-background-radius: 10; -fx-border-radius: 10;">
         <font>
            <Font size="15.0"/>
         </font>
      </TextField>
      <Button alignment="CENTER" layoutX="429.0" layoutY="16.0" mnemonicParsing="false"
              onAction="#onSearchButtonPressed" style="-fx-background-color: #2a0033;" text="Search" textFill="#ddd9d9">
         <font>
            <Font size="14.0"/>
         </font>
      </Button>
      <HBox layoutX="112.0" layoutY="57.0" prefHeight="333.0" prefWidth="378.0">
         <children>
            <VBox fx:id="exerciseNameVBox" alignment="TOP_CENTER" prefHeight="342.0" prefWidth="291.0" spacing="17.0">
               <padding>
                  <Insets top="10.0"/>
               </padding>
            </VBox>
            <VBox fx:id="addButtonVBox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0"/>
         </children>
      </HBox>
      <Button alignment="CENTER" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBackButtonPressed"
              prefHeight="29.0" prefWidth="54.0"
              style="-fx-background-color: #2a0033; -fx-border-color: #9643a9; -fx-background-radius: 10; -fx-border-radius: 10;"
              text="Back" textFill="#ddd9d9"/>
   </children>
</AnchorPane>
